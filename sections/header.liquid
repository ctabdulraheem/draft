<section data-section-id="{{ section.id }}" data-section-type="header">
  <header>
    {% include 'navigation' %}
  </header>
</section>

{% schema %}
  {
    "name": "Header",
    "settings": [
      {
        "type": "image_picker",
        "id": "logo_picker",
        "label": "Logo Image"
      }, {
        "type": "range",
        "id": "logo_size",
        "min": "10",
        "max": "200",
        "unit": "px",
        "step": 1,
        "label": "Custom Logo Size",
        "default": "114"
      }
    ]
  }
{% endschema %}